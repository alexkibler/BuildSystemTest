{
  "name": "build-system-test",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build-Debug": "npm -v && ng lint && ng build",
    "build-Release": "npm -v && ng build --prod",
    "continuous-dev": "npm -v && ng build -w -op wwwroot",
    "lint": "npm -v && ng lint",
    "postinstall": "npm run fixDC",
    "fixDC": "replace-in-file \"define([\"\"d3\"\", \"\"crossfilter\"\"], _dc);\" \"define([\"\"d3\"\", \"\"crossfilter2\"\"], _dc);\" \"./node_modules/dc/dc.js\" [--isRegex]"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "dc": "^2.0.2",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "^4.0.0",
    "@types/dc": "0.0.23",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "replace-in-file": "^2.5.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
